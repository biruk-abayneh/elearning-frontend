{
  "expo": {
    "name": "Nerd Out",
    "slug": "nerd_out",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/no-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/no-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.nerdout.app",
      "jsEngine": "hermes"
    },
    "android": {
      "package": "com.nerdout.app",
      "versionCode": 1,
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "foregroundImage": "./assets/no-icon.png",
        "backgroundColor": "#0F172A"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "nerdout"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/no-icon.png"
    },
    "scheme": "nerdout",
    "plugins": [
      "expo-asset",
      "expo-web-browser",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/PlusJakartaSans-Bold.ttf",
            "./assets/fonts/Poppins-Regular.ttf",
            "./assets/fonts/Poppins-Bold.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "eacb503c-e011-4159-b9ed-9270d6260d18"
      }
    }
  }
}
